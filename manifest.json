{	
	"manifest_version": 3,
	"name": "Context Menu Search Engines",
	"description": "Highlight text and choose a search engine to query in context menu",
	"version": "1.0.0",
    "background": {
       "service_worker": "script.js"
    },
    "permissions": [ "contextMenus" ],
	"icons" : {
		"16": "icons/16_icon.png",
		"32": "icons/32_icon.png",
		"48": "icons/48_icon.png",
		"128": "icons/128_icon.png"
	}
 }
 
